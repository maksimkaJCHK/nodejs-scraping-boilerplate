{
  "name": "nodejs-scraping-boilerplate",
  "version": "0.4.0",
  "description": "scraping with node.js",
  "main": "index.js",
  "scripts": {
    "scrapy:static": "node ./src/spiders/khazinSpider.js",
    "scrapy:browser": "node ./src/spiders/browserExampleSpider.js",
    "scrapy:shop": "node ./src/spiders/shopScriping.js",
    "scrapy:documents": "node ./src/spiders/documents.js",
    "analitics": "node ./src/analitics/shop.js",
    "server:static": "nodemon ./src/server/index.js",
    "server:react": "nodemon ./src/server-react/express-config-compiled.js",
    "compile:react:ssr": "rollup -c ./src/server-react/rollup-ssr.js -w",
    "compile:react:front:dev": "rollup -c ./src/server-react/rollup-front-dev.js -w",
    "compile:react:front:build": "rollup -c ./src/server-react/rollup-front-build.js"
  },
  "keywords": [
    "node",
    "node.js",
    "node.js scraping",
    "scraping",
    "js scraping",
    "javascript scraping",
    "nodejs scraping boilerplate",
    "scraping boilerplate",
    "javascript scraping boilerplate",
    "js scraping boilerplate",
    "web scraping boilerplate",
    "web scraping boilerplate for js",
    "react ssr",
    "rollup compile in es6 to cjs module",
    "parsing books shop",
    "парсинг книжных интернет-магазинов"
  ],
  "author": "Maksim Zhuchkov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "app-root-path": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "cheerio": "^1.0.0-rc.12",
    "cllc": "^1.2.0",
    "core-js": "^3.36.1",
    "express": "^4.18.2",
    "less": "^4.2.0",
    "log": "^6.3.1",
    "needle": "^3.2.0",
    "node-sass": "^9.0.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "postcss-url": "^10.1.3",
    "pug": "^3.0.2",
    "puppeteer": "20.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "redux-toolkit": "^1.1.2",
    "rollup": "^2.79.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "tress": "^1.1.4"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.2",
    "axios": "^1.6.8",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.1",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-sizes": "^1.0.6"
  }
}
